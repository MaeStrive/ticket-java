<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ahao.ticket.mapper.CategoryMapper">

    <resultMap id="BaseResultMap" type="com.ahao.ticket.domain.Category">
            <id property="id" column="Id" jdbcType="INTEGER"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="iconPath" column="icon_path" jdbcType="VARCHAR"/>
            <result property="sequence" column="sequence" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        Id,name,icon_path,
        sequence
    </sql>
</mapper>
