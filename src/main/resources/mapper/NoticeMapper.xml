        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ahao.ticket.mapper.NoticeMapper">

    <resultMap id="BaseResultMap" type="com.ahao.ticket.domain.Notice">
            <id property="id" column="id" jdbcType="INTEGER"/>
